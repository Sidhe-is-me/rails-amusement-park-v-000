<%= form_for @user do  |f| %>
<%= f.label :name%>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password%>
<br></br>
<%= f.label :nausea %>
<%= f.number_field :nausea %>
<%= f.label :happiness %>
<%= f.number_field :happiness %>
<%= f.label :height %>
<%= f.number_field  :height %>
<%= f.label :tickets %>
<%= f.number_field  :tickets %>
<br></br>
<%= f.label :admin, "Check box if you are a theme park owner" %>
   <%= f.check_box :admin%>
<br></br>
<%= f.submit "Create User" %>
<% end %>
